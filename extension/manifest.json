{
  "name": "AI Assistant Disabler",
  "description": "This is extension that allows you to turn off AI assistant hints",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "images/logo-16.png",
    "32": "images/logo-32.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/logo-16.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  },
  "content_scripts": [
    {
      "js": ["scripts/yandex-gpt-disabler.js"],
      "matches": [
        "https://yandex.ru/search/*"
      ]
    }
  ]
}
